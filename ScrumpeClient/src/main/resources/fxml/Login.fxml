      </PasswordField>
      <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" maxWidth="1.7976931348623157E308" text="Forgot Password" textAlignment="RIGHT">
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </Label>
      <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleButtonAction" text="Button" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
